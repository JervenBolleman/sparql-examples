<https://www.wikidata.org/#query-d0749bcc20e6eb170dbedbf3b5fa17ac> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#This query favours people whose names start ABC &c over those starting XYZ &c.
  SELECT ?person ?sitelink ?article
                 WHERE{
                   SERVICE bd:sample {
                     ?person wdt:P21 wd:Q6581097 .
                     bd:serviceParam bd:sample.limit 2000 .
                   } 
                   ?person wdt:P31 wd:Q5 .
                   ?sitelink schema:about ?person ;
                             schema:isPartOf <https://en.wikipedia.org/> ;
                             schema:name ?article .
                 }
  ORDER BY ?article LIMIT 100""";
