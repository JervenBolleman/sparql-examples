<https://www.wikidata.org/#query-5ccaedadf651712ce77f209e3f9ac144> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?language ?languageLabel (COUNT(?language) AS ?count)
WHERE
{
  ?country wdt:P31 wd:Q6256.
  ?country wdt:P37 ?language.
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}
GROUP BY ?language ?languageLabel
ORDER BY DESC(?count)""";
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:Request_a_query/Archive/2020/04>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/> .
