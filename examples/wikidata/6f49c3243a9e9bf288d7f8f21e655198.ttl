<https://www.wikidata.org/#query-6f49c3243a9e9bf288d7f8f21e655198> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?author ?authorLabel ?authorDescription (COUNT(?letter) as ?count)
WHERE {
  ?letter wdt:P50 ?author;
   wdt:P31 wd:Q133492.
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\" }
} GROUP BY ?author ?authorLabel ?authorDescription
ORDER BY DESC(?count)""";
  rdfs:comment "Properties: author (P50)   , instance of (P31)   "@en;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:MartinPoulter/queries>;
