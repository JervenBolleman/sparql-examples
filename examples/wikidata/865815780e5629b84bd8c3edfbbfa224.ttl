<https://www.wikidata.org/#query-865815780e5629b84bd8c3edfbbfa224> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#Mines in Serbia
#Creates a list of mines in Serbia
SELECT DISTINCT ?rudnik ?rudnikLabel 
WHERE {
  ?rudnik wdt:P31 wd:Q820477 ;
          ?range wd:Q403 .
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"sr,[AUTO_LANGUAGE],en\". }
} LIMIT 400""";
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:WikiProject_Mining_Sites/Queries>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/> .
