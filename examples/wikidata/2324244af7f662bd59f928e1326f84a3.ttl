<https://www.wikidata.org/#query-2324244af7f662bd59f928e1326f84a3> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#People with ORCID ID, occupation researcher, given name, no family name or sex 
SELECT DISTINCT ?item ?itemLabel

WHERE {?item wdt:P31 wd:Q5;
             wdt:P106 wd:Q1650915;
             wdt:P496 [ ];
             wdt:P735 [ ].
       MINUS {?item wdt:P21 [ ]}.
       MINUS {?item wdt:P734 [ ]}
       SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
    }""";
