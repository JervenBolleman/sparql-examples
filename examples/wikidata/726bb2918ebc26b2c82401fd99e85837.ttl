<https://www.wikidata.org/#query-726bb2918ebc26b2c82401fd99e85837> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?country ?countryLabel ?religion ?religionLabel ?proportion ?percentage WHERE {
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
  ?country wdt:P31 wd:Q3624078.
  OPTIONAL
  {
    ?country p:P140 ?stm.
    ?stm ps:P140 ?religion.
    OPTIONAL
    {
      ?stm pq:P1107 ?proportion.
      BIND (?proportion * 100 AS ?percentage)
    }
  }
}""";
