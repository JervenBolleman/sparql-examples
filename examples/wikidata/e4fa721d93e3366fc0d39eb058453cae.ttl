<https://www.wikidata.org/#query-e4fa721d93e3366fc0d39eb058453cae> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?software ?softwareLabel (COUNT(?article) AS ?count)

WHERE {
  ?article wdt:P31 wd:Q13442814.
  ?article wdt:P2283 ?software .
            
  SERVICE wikibase:label {
		bd:serviceParam wikibase:language \"en\" .
    
}
  }
GROUP BY ?software ?softwareLabel 
ORDER BY DESC(?count)""";
