<https://www.wikidata.org/#query-353c559f72d606be6219e0a04aa74429> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?hersteller ?herstellerLabel (SUM(?produziert) AS ?gesamtProduziert)
WHERE
{
  ?modell wdt:P31?/wdt:P279* wd:Q12796;
         wdt:P176 ?hersteller;
         wdt:P1092 ?produziert.
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE]\". }
}
GROUP BY ?hersteller ?herstellerLabel
ORDER BY DESC(?produziert)""";
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:SPARQL_tutorial/de>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/> .
