ex:sparqlrdf/User:AllyD/sandbox/snippet_36.ttl a sh:SPARQLExecutable,
        sh:SPARQLSelectExecutable ;
    rdfs:comment "Sparql query from file sparqlrdf/User:AllyD/sandbox/snippet_36.ttl"^^rdf:HTML ;
