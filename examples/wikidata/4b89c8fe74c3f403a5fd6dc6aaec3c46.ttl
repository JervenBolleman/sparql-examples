<https://www.wikidata.org/#query-4b89c8fe74c3f403a5fd6dc6aaec3c46> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?pays ?paysLabel ?PIB__PPA__par_habitant ?date with {
  select ?pays (max(?date) as ?date)
  WHERE {
   ?pays wdt:P31 wd:Q6256.
   ?pays p:P2299 [pq:P585 ?date]. 
    }  group by ?pays } as %i
where 
{
  include %i
  ?pays p:P2299 [ps:P2299 ?PIB__PPA__par_habitant; pq:P585 ?date]. 
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"fr\".  }
}""";
