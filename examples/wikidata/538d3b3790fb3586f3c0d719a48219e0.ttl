<https://www.wikidata.org/#query-538d3b3790fb3586f3c0d719a48219e0> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """select (count(?male)/count(?item) * 100 as ?percentman) (count(?female)/count(?item) * 100 as ?percentwoman) (count(?other)/count(?item) * 100 as ?percentother) where { 
    ################################################################################
    # this part defines the dataset you want to query, put whatever you want in it
    # example:
    #?item wdt:P31 wd:Q5 .
    #but this could be
     {
     select ?item where {
       ?item wdt:P31 wd:Q5 .
     } limit 500000  }
    ################################################################################

    optional { bind(\"1\" as ?male) . ?item wdt:P21 wd:Q6581097 .}
    optional { bind(\"1\" as ?female) . ?item wdt:P21 wd:Q6581072 . }
    optional { bind(\"1\" as ?other) . ?item wdt:P21 ?gender filter (?gender not in (wd:Q6581097, wd:Q6581072) ). }
    
}
    
}""";
