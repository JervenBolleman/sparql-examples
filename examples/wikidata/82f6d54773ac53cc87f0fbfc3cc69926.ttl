<https://www.wikidata.org/#query-82f6d54773ac53cc87f0fbfc3cc69926> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?item ?itemLabel ?age WHERE {
  { ?item wdt:P106 wd:Q2906862. }
  UNION
  { ?item wdt:P106 wd:Q17125263. }
  UNION
  { ?item wdt:P106 wd:Q131524. }
  FILTER(NOT EXISTS { ?article schema:about ?item. })
  ?item wdt:P569 ?dob.
  BIND((YEAR(NOW())) - (YEAR(?dob)) AS ?age)
  FILTER(?age < 20 )
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
 }""";
