@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix schema: <https://schema.org/> .

<https://www.wikidata.org/#query-1ed68f7039c3ae94d6623f1ec7f7e52c> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT $municipality $municipalityLabel (COUNT($population) AS $count) 
  {
  $municipality wdt:P31 wd:Q262166;
                wdt:P1082 $population.

  SERVICE wikibase:label {bd:serviceParam wikibase:language \"de,en\"}
  }
GROUP BY $municipality $municipalityLabel
HAVING ($count > 1)
ORDER BY $count $municipalityLabel""";
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Faring/Maintenance>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/> .
