<https://www.wikidata.org/#query-2a3efd7abb74ee5090526e74f36b465b> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?lexemeId ?lemma ?dicoentry ?startpage ?endpage WHERE {
  ?lexemeId dct:language wd:Q150 ; wikibase:lemma ?lemma ; p:P1343 ?dico.
  ?dico ps:P1343 wd:Q115682499 ; pq:P304 ?page ; pq:P1932 ?dicoentry.
  BIND(xsd:integer(REPLACE(?page, \"-[0-9]*\", \"\")) AS ?startpage)
  BIND(xsd:integer(REPLACE(?page, \"[0-9]*-\", \"\")) AS ?endpage) 
}
ORDER BY ASC(?startpage)""";
