<https://www.wikidata.org/#query-0225dcdd63f0121de0d84b15490b1d3c> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?item ?itemLabel ?start ?LonLat WHERE {
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],de\". }
  ?item p:P1343 ?statement.
  ?statement ps:P1343 wd:Q89404596.
  OPTIONAL {
    ?statement pq:P1545 ?start.
    FILTER(!(ISBLANK(?start)))
  }
  OPTIONAL {
    ?item wdt:P625 ?LonLat .
    FILTER(!(ISBLANK(?start)))
  }
}
ORDER BY (?itemLabel)""";
