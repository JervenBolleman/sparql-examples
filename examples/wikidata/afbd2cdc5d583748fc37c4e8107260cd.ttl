<https://www.wikidata.org/#query-afbd2cdc5d583748fc37c4e8107260cd> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """# Elenco di poeti italiani nati nel 900 in ordine di data di nascita, con eventuale pagina di Wikipedia
SELECT ?item ?itemLabel ?ddn ?cc ?wkp WHERE {
  ?item wdt:P31 wd:Q5;
    wdt:P106 wd:Q49757;
    wdt:P569 ?ddn;
    wdt:P27 wd:Q38.
  OPTIONAL { ?item wdt:P8248 ?cc. }
  OPTIONAL {
    ?wkp schema:about ?item.
    FILTER(REGEX(STR(?wkp), \"it.wikipedia.org\"))
  }
  FILTER((?ddn >= \"1900-01-01T00:00:00Z\"^^xsd:dateTime) && (?ddn <= \"1999-12-31T00:00:00Z\"^^xsd:dateTime))
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}
ORDER BY (?ddn)""";
