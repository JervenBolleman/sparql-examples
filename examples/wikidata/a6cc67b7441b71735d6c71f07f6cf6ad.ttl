<https://www.wikidata.org/#query-a6cc67b7441b71735d6c71f07f6cf6ad> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?item ?itemLabel ?instanceLabel ?metricalPattern {
  {
    SELECT ?item (SAMPLE(?instance) AS ?instance) (SAMPLE(?metricalPattern) AS ?metricalPattern) {
      ?item wdt:P2552 ?metricalPattern;
            wdt:P31 ?instance .
