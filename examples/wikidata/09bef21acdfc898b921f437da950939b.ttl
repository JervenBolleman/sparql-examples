@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix schema: <https://schema.org/> .

<https://www.wikidata.org/#query-09bef21acdfc898b921f437da950939b> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#defaultView:Table
SELECT ?item ?title ?yiwiki ?dewiki ?istEin ?image (GROUP_CONCAT(DISTINCT ?gsprache; SEPARATOR = \", \") AS ?sprachen) ?LTAid (URI(CONCAT(\"https://www.librarything.com/author/\", ?LTAid)) AS ?LTAurl) WHERE {
  {
    ?item wdt:P31 ?instanz.
    ?yiwiki schema:about ?item;
      schema:isPartOf <https://yi.wikipedia.org/>;
      schema:name ?title.
    OPTIONAL { ?item wdt:P7400 ?LTAid. }
    OPTIONAL {
      ?dewiki schema:about ?item;
        schema:isPartOf <https://de.wikipedia.org/>;
        schema:name ?de_title.
    }
    OPTIONAL { ?item wdt:P1412 ?sprache. }
    OPTIONAL { ?item wdt:P18 ?image. }
    FILTER((?instanz in ( wd:Q5, wd:Q43229)) && (!(BOUND(?LTAid))))
    SERVICE wikibase:label {
      bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\".
      ?sprache rdfs:label ?gsprache.
      ?instanz rdfs:label ?istEin
    }
  }
}
GROUP BY ?item ?title ?yiwiki ?dewiki ?LTAid ?istEin ?image
ORDER BY (?title)""";
