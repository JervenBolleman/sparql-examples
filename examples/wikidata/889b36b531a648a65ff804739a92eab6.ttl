@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix schema: <https://schema.org/> .

<https://www.wikidata.org/#query-889b36b531a648a65ff804739a92eab6> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#les éléments dans Wikidata disposant d'une ID SAPA et étant une organisation par type
SELECT ?type ?typeLabel (COUNT(?type) AS ?count) 
WHERE {
  ?item (wdt:P31/(wdt:P279*)) wd:Q43229.
  hint:Prior hint:gearing \"forward\".
  ?item wdt:P8974 [].
  hint:Prior hint:runFirst true.
  ?item wdt:P31 ?type.
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}
GROUP BY ?typeLabel ?type ?count
ORDER BY DESC (?count)""";
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:SAPA_bdc/SPARQL>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/> .
