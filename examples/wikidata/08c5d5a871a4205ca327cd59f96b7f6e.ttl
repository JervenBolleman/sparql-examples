SELECT ?item ?itemLabel ?movementLabel
WHERE {
  { ?item wdt:P135 ?instance .
  ?instance wdt:P279 wd:Q1100736 .}
  ?item wdt:P135 ?movement .
	SERVICE wikibase:label { bd:serviceParam wikibase:language \"it\" }    
} ORDER BY ?movementLabel""";
