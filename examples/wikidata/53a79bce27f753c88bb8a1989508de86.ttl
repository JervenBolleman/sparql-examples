<https://www.wikidata.org/#query-53a79bce27f753c88bb8a1989508de86> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?item ?image ?label WITH {
  SELECT ?item (SAMPLE(?p18) AS ?image) {
    ?item wdt:P31/wdt:P279* wd:Q31629 .
    OPTIONAL { ?item wdt:P18 ?p18 } .
  } GROUP BY ?item
} AS %subquery WHERE {
  INCLUDE %subquery .
  ?item rdfs:label ?label .
  FILTER ( LANG( ?label ) = \"en\" ) .
  #FILTER ( STRSTARTS( LCASE( ?label ), \"bo\" ) ) .
}""";
