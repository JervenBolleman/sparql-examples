<https://www.wikidata.org/#query-226a6eb7e268d70ca6bf1f09575023be> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?item ?itemLabel (GROUP_CONCAT(DISTINCT ?occupationLabel; SEPARATOR=\", \") AS ?occupations) (GROUP_CONCAT(DISTINCT ?countryLabel; SEPARATOR=\", \") AS ?countries) {
  VALUES ?occupation {wd:Q19509201} . 
   ?item wdt:P31 wd:Q5;
               wdt:P27 ?country;
               wdt:P27 ?target_country;
               wdt:P106 ?occupation .
    FILTER NOT EXISTS {
    ?article schema:about ?item .
    ?article schema:isPartOf <https://fr.wikipedia.org/> .
  }
   SERVICE wikibase:label {
       bd:serviceParam wikibase:language \"fr,en\" .
       ?item rdfs:label ?itemLabel .
       ?occupation rdfs:label ?occupationLabel .
       ?country rdfs:label ?countryLabel .
   } .
} GROUP BY ?item ?itemLabel ORDER BY ?countries""";
