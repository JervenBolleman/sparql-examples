  sh:select """SELECT
  ?language
  ?languageLabel
  ?total
  ?without
  ?with
  ?ratio
WITH {
  SELECT ?language {
    VALUES ?language {
      wd:Q1321
    }
  }
} AS %0
WITH {
  SELECT ?language (COUNT(*) AS ?with) {
   INCLUDE %0.
    [] dct:language ?language;
       ontolex:lexicalForm [wdt:P443 []]
  }
  GROUP BY ?language
} AS %1
WITH {
  SELECT ?language (COUNT(*) AS ?total) {
    INCLUDE %0.
    [] dct:language ?language;
       ontolex:lexicalForm [].
  }
  GROUP BY ?language
} AS %2
{
  INCLUDE %1.
  INCLUDE %2.
  BIND((?total - ?with) AS ?without)
  BIND(CONCAT(STR((?with/?total)), \"Â %\") AS ?ratio)
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\". }
}
ORDER BY DESC(?without)""";
