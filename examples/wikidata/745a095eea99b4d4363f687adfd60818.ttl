<https://www.wikidata.org/#query-745a095eea99b4d4363f687adfd60818> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """# Abfragen von Property Qualifier (Start- und Endzeit)
SELECT ?item ?itemLabel 
#?itemDescription 
?hasc ?nuts ?startime ?endtime 
#?capital 
WHERE 
{
  ?item wdt:P17 wd:Q183.
  ?item wdt:P131 wd:Q1202.
  OPTIONAL{?item wdt:P8119 ?hasc. }
  ?item wdt:P605  ?nuts.
  OPTIONAL{?item p:P605 [  pq:P580 ?starttime ]  . }    # pq Property Qualifier
  OPTIONAL{?item p:P605 [  pq:P582 ?endtime ]  . }
  #?item wdt:P36 ?capital.  
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}
order by ?endtime ?itemLabel""";
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Stefan_K%C3%BChn/SPARQL>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/> .
