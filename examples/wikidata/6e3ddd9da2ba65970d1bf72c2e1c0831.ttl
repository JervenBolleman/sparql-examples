      ?ship p:P137 ?statement0.
      ?statement0 (ps:P137/(wdt:P279*)) wd:Q172771.
      {
        ?ship p:P31 ?statement1.
        ?statement1 (ps:P31/(wdt:P279*)) wd:Q852190.
      }
      UNION
      {
        ?ship p:P366 ?statement2.
