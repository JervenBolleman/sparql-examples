  ?participant wdt:P31 wd:Q5.
  ?participant wdt:P1344 wd:Q673014.
  ?participant wdt:P27 ?citizenship.
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"hr,fr\". }
}
GROUP BY ?citizenshipLabel
ORDER BY DESC(?count)""";
