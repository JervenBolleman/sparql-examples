<https://www.wikidata.org/#query-876de970e67a3c42182809e66723d84c> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?chiesa ?chiesaLabel ?coordinate_geografiche WHERE {
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"it,en\". }
  ?chiesa (wdt:P31/(wdt:P279*)) wd:Q16970;
    (wdt:P131/wdt:P131/wdt:P131) wd:Q16289.
  FILTER NOT EXISTS { ?chiesa wdt:P625 ?coordinate_geografiche. }
}""";
