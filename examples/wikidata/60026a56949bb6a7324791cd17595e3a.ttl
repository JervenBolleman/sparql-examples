<https://www.wikidata.org/#query-60026a56949bb6a7324791cd17595e3a> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#title: Ministries of Bhutan (should be 9)
SELECT ?ministry ?ministryLabel WHERE {
  ?ministry wdt:P31 wd:Q192350;
    wdt:P1001 wd:Q917.
  
  FILTER NOT EXISTS { 
    ?ministry p:P576 ?dissolution.  # Exclude those with a dissolution statement
  }
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\". }
}""";
