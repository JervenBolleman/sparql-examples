SELECT ?item ?itemLabel ?sitelink
WHERE 
{
  ?item wdt:P31 wd:Q5 .
  ?item wdt:P21 wd:Q48270 .
  ?sitelink schema:about ?item . 
  ?sitelink schema:inLanguage \"nl\" .
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}""";
