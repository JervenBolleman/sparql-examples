<https://www.wikidata.org/#query-f8f1f826646f4a3effa287993d84761b> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?author ?authorLabel ?thesis ?typeLabel ?url WHERE {
   ?author wdt:P1026 ?thesis .
   ?thesis wdt:P31 ?type 
  OPTIONAL { ?thesis wdt:P953 ?url }
   SERVICE wikibase:label {
     bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\"
   }
} ORDER BY ?typeLabel""";
  rdfs:comment "Properties: academic thesis (P1026)   , instance of (P31)   , full work available at URL (P953)   "@en;
