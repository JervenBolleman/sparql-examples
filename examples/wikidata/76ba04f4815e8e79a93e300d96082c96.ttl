<https://www.wikidata.org/#query-76ba04f4815e8e79a93e300d96082c96> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """# Who are things in Hungary named after in descending order
SELECT ?nameSake ?nameSakeLabel ?count
WHERE
{
  {
    SELECT ?nameSake (COUNT(?item) AS ?count)
    WHERE
    {
        {
          ?item wdt:P131* wd:Q28 .
        }
        UNION
        {
          ?item wdt:P17 wd:Q28 .
        }
        ?item wdt:P138 ?nameSake .
    }
    GROUP BY ?nameSake
  }
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"hu,en\" }
}
ORDER BY DESC (?count)""";
