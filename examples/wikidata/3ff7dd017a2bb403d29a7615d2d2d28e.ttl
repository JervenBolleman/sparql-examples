@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix schema: <https://schema.org/> .

<https://www.wikidata.org/#query-3ff7dd017a2bb403d29a7615d2d2d28e> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT DISTINCT ?propLabel ?p
WHERE {
  ?s wdt:P364 ?originalLanguage .
  ?s wdt:P407 ?variantWorkLanguage .
  OPTIONAL {
    ?s ?p ?o .
  }
  ?prop wikibase:directClaim ?p .
  FILTER (?originalLanguage != ?variantWorkLanguage)
  FILTER (STRSTARTS(STR(?p), STR(wdt:)))
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\" }
}
ORDER BY ?propLabel""";
  rdfs:comment "Properties: original language of film or TV show (P364)   , language of work or name (P407)   "@en;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Realworldobject/ShowTell>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/> .
