ex:sparqlrdf/Wikidata_talk:Status_updates/Next/Archive_1/snippet_1.ttl a sh:SPARQLExecutable,
        sh:SPARQLSelectExecutable ;
    rdfs:comment "Sparql query from file sparqlrdf/Wikidata_talk:Status_updates/Next/Archive_1/snippet_1.ttl"^^rdf:HTML ;
