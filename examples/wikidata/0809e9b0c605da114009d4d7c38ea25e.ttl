<https://www.wikidata.org/#query-0809e9b0c605da114009d4d7c38ea25e> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?item ?itemLabel ?illiterate ?literate ?population (?population - ?illiterate - ?literate as ?diff)
WHERE 
{
  ?item wdt:P6498 ?illiterate .
  ?item wdt:P6499 ?literate . 
  ?item wdt:P1082 ?population .
  ?item p:P31/ps:P31 wd:Q6256.   
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". } 
}""";
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:Request_a_query/Archive/2022/10>;
