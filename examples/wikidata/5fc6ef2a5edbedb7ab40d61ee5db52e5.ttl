<https://www.wikidata.org/#query-5fc6ef2a5edbedb7ab40d61ee5db52e5> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#Retrieve all mature miRNAs and their targets (names and NCBI entrez gene ID)
SELECT DISTINCT ?mirna ?mirnaLabel ?gene ?geneLabel ?entrez WHERE {
  ?mirna wdt:P31 wd:Q23838648 ;
         wdt:P128 ?gene .
  ?gene wdt:P351 ?entrez .
  
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\". }
}""";
  rdfs:comment "Properties: instance of (P31)   , regulates (molecular biology) (P128)   , Entrez Gene ID (P351)   "@en;
