<https://www.wikidata.org/#query-d2de894e9f73ada307ce79fdc4b3ae71> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?item ?itemLabel ?cantic ?ref_named_as ?url
WHERE 
{
  ?item wdt:P1273 ?cantic.
  ?item p:P570 ?stat .
  ?stat prov:wasDerivedFrom ?stat2 .
  ?stat2 pr:P854 ?url.
  ?stat2 pr:P1810 ?ref_named_as.
  filter (contains(str(?url),\"cantic\"))
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}""";
