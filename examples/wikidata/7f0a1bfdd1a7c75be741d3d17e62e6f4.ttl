WHERE 
{
  ?item p:P1733 [ ps:P1733 ?steam_ID ; prov:wasDerivedFrom [pr:P143 wd:Q17013880 ] ].
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
} order by ?itemLabel""";
