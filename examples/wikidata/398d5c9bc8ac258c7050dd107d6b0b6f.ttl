@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix schema: <https://schema.org/> .

<https://www.wikidata.org/#query-398d5c9bc8ac258c7050dd107d6b0b6f> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT DISTINCT ?item ?taxonName ?taxonRankLabel WHERE {
      ?item wdt:P225 ?taxonName .
      ?item wdt:P105 ?taxonRank .
      ?item (wdt:P171)* ?higherParent .  
      ?higherParent wdt:P944 wd:Q14920640.
      SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE]\". }
}
ORDER BY ASC(?taxonName)""";
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Succu/SPARQL>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/> .
