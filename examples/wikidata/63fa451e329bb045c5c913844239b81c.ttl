<https://www.wikidata.org/#query-63fa451e329bb045c5c913844239b81c> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#defaultView:BarChart
SELECT (CONCAT(STR(?genderLabel), \" / \", STR(?orientationLabel)) AS ?lab) (COUNT(*) AS ?count) WHERE {
  SELECT ?genderLabel ?orientationLabel WHERE {
    ?item wdt:P31 wd:Q5;
      wdt:P21 ?gender;
      wdt:P91 ?orientation.
    SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
  }
}
GROUP BY ?genderLabel ?orientationLabel""";
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Misc/Queries>;
