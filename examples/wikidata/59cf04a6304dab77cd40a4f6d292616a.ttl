<https://www.wikidata.org/#query-59cf04a6304dab77cd40a4f6d292616a> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#title: Query for all Ministries of Japan (should be 42)
SELECT ?item ?itemLabel WHERE {
  ?item wdt:P31 wd:Q15409298 ;
        wdt:P17 wd:Q17.
  MINUS { ?item wdt:P576 [] }
  MINUS { ?item wdt:P1366 [] }
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"ja,en\". }
}
ORDER BY ?itemLabel""";
