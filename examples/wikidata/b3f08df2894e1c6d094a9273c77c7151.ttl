<https://www.wikidata.org/#query-b3f08df2894e1c6d094a9273c77c7151> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?land (MAX(?einwohnerzahl) AS ?maxEinwohnerzahl)
WHERE
