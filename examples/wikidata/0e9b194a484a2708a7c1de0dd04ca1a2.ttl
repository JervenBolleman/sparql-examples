<https://www.wikidata.org/#query-0e9b194a484a2708a7c1de0dd04ca1a2> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """# Find all mineral type localities and sort them by most minerals defined
SELECT ?locality ?name (COUNT(DISTINCT ?mineral) AS ?count)
WHERE
{
    ?mineral wdt:P579 ?status .
    ?mineral wdt:P2695 ?locality .          # Where a locality statement exists
  
    SERVICE wikibase:label {
        bd:serviceParam wikibase:language \"en\" .
        ?locality rdfs:label ?name
    }
}
GROUP BY ?locality ?name
ORDER BY DESC(?count)""";
