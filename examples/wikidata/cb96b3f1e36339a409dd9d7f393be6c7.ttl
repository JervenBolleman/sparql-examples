<https://www.wikidata.org/#query-cb96b3f1e36339a409dd9d7f393be6c7> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """# What languages do people speak that speak Armenian
#defaultView:BubbleChart
SELECT ?language ?languageLabel ?count WHERE {
  {
    SELECT ?language (COUNT(?item) AS ?count) WHERE {
      ?item wdt:P1412 wd:Q8785.
      ?item wdt:P1412 ?language.
    }
    GROUP BY ?language
  }
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"hy\". }
}
ORDER BY DESC(?count)""";
