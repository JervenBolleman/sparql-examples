<https://www.wikidata.org/#query-aa14df3a20124cc6ab331f1bbeb7e075> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#Orte mit HOV-Code und Beziehung zu anderen Normdateien
SELECT DISTINCT ?Orte ?OrteLabel ?HOV_Code ?extId ?extIdLabel ?externe_ID WHERE {
  ?Orte wdt:P7046 ?HOV_Code.
  OPTIONAL {
    ?Orte ?p ?externe_ID.
    ?extId wikibase:directClaim ?p;
      (wdt:P31/(wdt:P279*)) wd:Q18614948.
  }
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}
ORDER BY ?Orte""";
