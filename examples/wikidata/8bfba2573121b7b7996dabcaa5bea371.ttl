<https://www.wikidata.org/#query-8bfba2573121b7b7996dabcaa5bea371> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT distinct ?item ?itemLabel ?designation ?designationLabel WHERE 
{
  ?item wdt:P1435 wd:Q19558910 . 
  ?item p:P1435 ?name . 
  OPTIONAL {?name ps:P1435 []; pq:P2868 ?designation .}
 SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}
order by ?designation""";
