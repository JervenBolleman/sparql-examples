<https://www.wikidata.org/#query-322e61d1e9accfc08a609e4c2464ba02> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?template ?templateLabel ?count WITH {
  SELECT ?template ?count WHERE {
    ?template wikibase:sitelinks ?count. hint:Prior hint:rangeSafe true.
    FILTER(?count >= 250)
  }
  ORDER BY DESC(?count)
  LIMIT 100
} AS %results WHERE {
  INCLUDE %results.
  ?template wdt:P31/wdt:P279* wd:Q11266439.
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}
ORDER BY DESC(?count)""";
