@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix schema: <https://schema.org/> .

<https://www.wikidata.org/#query-bb00a377a27b70630975bf3be7ea1f67> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?fabricante ?fabricanteLabel (SUM(?produzido) AS ?totalProduzido)
WHERE
{
  ?modelo wdt:P31?/wdt:P279* wd:Q12796;
         wdt:P176 ?fabricante;
         wdt:P1092 ?produzido.
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE]\". }
}
GROUP BY ?fabricante ?fabricanteLabel
ORDER BY DESC(?produzido)""";
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:SPARQL_tutorial/pt-br>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/> .
