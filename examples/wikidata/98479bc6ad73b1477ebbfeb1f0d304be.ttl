<https://www.wikidata.org/#query-98479bc6ad73b1477ebbfeb1f0d304be> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT DISTINCT ?item ?itemLabel 
WHERE {
  hint:Query hint:optimizer \"None\".
  SERVICE wikibase:mwapi {
    bd:serviceParam wikibase:api \"Search\";
                    wikibase:endpoint \"www.wikidata.org\";
                    mwapi:srsearch \"www.gamespot.com\".
    ?item wikibase:apiOutputItem mwapi:title .
  }
  filter(bound(?item))
  ?item wdt:P973|wdt:P953 ?url.
  filter(contains(str(?url),\"https://www.gamespot.com/companies\"))  
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}""";
