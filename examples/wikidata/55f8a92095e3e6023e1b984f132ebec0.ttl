<https://www.wikidata.org/#query-55f8a92095e3e6023e1b984f132ebec0> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?recipient ?recipientLabel ?letter ?letterLabel ?ws
WHERE {
  ?letter wdt:P1817 ?recipient.
    ?recipient wdt:P3429 []
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\" }
  OPTIONAL {?ws schema:about ?letter; schema:isPartOf <https://en.wikisource.org/> }
} ORDER BY ?recipientLabel""";
