<https://www.wikidata.org/#query-e686c4e651c0596b6f58d93926c1427e> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """# Distinct author gender distribution

PREFIX target: <http://www.wikidata.org/entity/Q3382012>

SELECT ?count ?gender ?genderLabel 
WITH {
  SELECT (COUNT(DISTINCT ?author) AS ?count) ?gender WHERE {
    ?work wdt:P1433 target: .
    ?work wdt:P50 ?author .
    ?author wdt:P21 ?gender .
  } 
  GROUP BY ?gender
} AS %result
WHERE {
  INCLUDE %result
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\" . } 
} 
 ORDER BY DESC(?count)""";
