<https://www.wikidata.org/#query-dc68d9b92a9ac3633086dcd1d7fdf58d> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#Trouve tous les éléments connus de Wikidata qui sont des \"archives communales\"
SELECT ?item ?itemLabel 
WHERE 
{
  ?item wdt:P31 wd:Q604177.
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}""";
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:WikiProject_Archival_Description/Use_cases>;
