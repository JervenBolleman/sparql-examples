<https://www.wikidata.org/#query-0a6d6a447cf14e21c9804cef3b71452f> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#List all available hospital types
SELECT ?hospital ?hospitalLabel ?hospitalType WHERE {
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
  ?hospital wdt:P279 ?hospitalType;
  FILTER (?hospitalType = wd:Q16917 || ?hospitalType = wd:Q4260475)
}""";
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:WikiProject_Hospitals/Report/East-Berlin>;
