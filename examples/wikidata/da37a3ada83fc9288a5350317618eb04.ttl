@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix schema: <https://schema.org/> .

<https://www.wikidata.org/#query-da37a3ada83fc9288a5350317618eb04> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?Personne ?PersonneLabel ?PersonneDescription ?datenais ?articleWikipédia ?lienswikipedia
WHERE 
{
  ?Personne wdt:P69 wd:Q3577716;
        wdt:P569 ?datenais.
  OPTIONAL {?articleWikipédia schema:about ?Personne .
    ?articleWikipédia schema:isPartOf <https://fr.wikipedia.org/>.
    }
  ?Personne wikibase:sitelinks ?lienswikipedia.
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],fr\". }
}
order by desc(?datenais)""";
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:FHd/Listes>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/> .
