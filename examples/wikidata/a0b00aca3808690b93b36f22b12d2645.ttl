<https://www.wikidata.org/#query-a0b00aca3808690b93b36f22b12d2645> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#title:VIAF appearing two times in the same item (= single-value violation)
SELECT DISTINCT ?item ?v1
WHERE {
  ?item p:P214 ?viaf1 .
  ?viaf1 ps:P214 ?v1 .
  ?item p:P214 ?viaf2 .
  ?viaf2 ps:P214 ?v2 .
  FILTER(?viaf1 != ?viaf2)
  FILTER(?v1 = ?v2)
}
LIMIT 10""";
