   ?basionymNameStatement ps:P225 ?basionym .
   ?basionymNameStatement pq:P405 wd:Q1043 . # Linn√©
   ?basionymNameStatement prov:wasDerivedFrom ?ref .
   ?ref pr:P248 ?sp .  
}
GROUP BY ?basionymItem ?basionym
ORDER BY ASC(?basionym)""";
