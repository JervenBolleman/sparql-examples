<https://www.wikidata.org/#query-38085639bce58f3e218cbfb158888e48> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#Minerals and their commons colors
#defaultView:Graph
SELECT DISTINCT ?mineral ?mineralLabel ?rgb ?color ?colorLabel
WHERE
{
  ?mineral wdt:P279* wd:Q7946 ;
           wdt:P462 ?color .
  ?color wdt:P465 ?rgb .
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\". }
}""";
