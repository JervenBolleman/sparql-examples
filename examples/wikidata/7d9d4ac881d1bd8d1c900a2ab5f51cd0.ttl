<https://www.wikidata.org/#query-7d9d4ac881d1bd8d1c900a2ab5f51cd0> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?format ?formatLabel (COUNT(?app) AS ?count)
WHERE {
  ?format wdt:P31/wdt:P279* wd:Q235557.
  ?app wdt:P1072 ?format.
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\". }
}

GROUP BY ?format ?formatLabel

ORDER BY DESC(?count)

LIMIT 50""";
