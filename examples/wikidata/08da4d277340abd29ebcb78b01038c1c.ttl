<https://www.wikidata.org/#query-08da4d277340abd29ebcb78b01038c1c> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT DISTINCT ?id  ?idLabel ?locLabel ?coord 
WHERE
{
    ?id wdt:P31/wdt:P279* wd:Q1210950 .
    ?id wdt:P17 wd:Q16 .
    ?id wdt:P131 ?region .
    ?region wdt:P131* wd:Q176 .
    ?id wdt:P131 ?loc .
    OPTIONAL {?id wdt:P625 ?coord }
    SERVICE wikibase:label {
            bd:serviceParam wikibase:language \"fr,en\" 
    }
}
ORDER BY (?idLabel) 
LIMIT 500""";
