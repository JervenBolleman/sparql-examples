  ?p wdt:P1416 ?w .
  ?w wdt:P1454 ?rechtsform .
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"de\" . }
} ORDER BY ASC(?rechtsformLabel) ASC(?wLabel) ASC(?pLabel)""";
