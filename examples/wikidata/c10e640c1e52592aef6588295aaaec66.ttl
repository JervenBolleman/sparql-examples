<https://www.wikidata.org/#query-c10e640c1e52592aef6588295aaaec66> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?item ?itemLabel ?start ?position ?positionLabel ?dob ?residence ?residenceLabel ?party ?partyLabel
WHERE 
{
  ?item p:P39 ?stat . 
  ?stat ps:P39 ?position . 
  ?position wdt:P279 wd:Q87008012.
  ?stat pq:P580 ?start .
  filter(year(?start)=2023)
  optional {?item wdt:P569 ?dob .}
  optional {?item wdt:P551 ?residence .}
  optional {?item wdt:P102 ?party .}
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],nn,en\". }
}""";
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:Request_a_query/Archive/2023/10>;
