WITH {
 
    SELECT DISTINCT ?author_name WHERE {
      { 
        SERVICE bd:sample { ?work wdt:P6104 wd:Q56241615 . bd:serviceParam bd:sample.limit 10000 }
        ?work wdt:P50 ?author .
        { ?author skos:altLabel ?author_name_. }
