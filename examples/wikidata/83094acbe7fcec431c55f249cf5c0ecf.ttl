<https://www.wikidata.org/#query-83094acbe7fcec431c55f249cf5c0ecf> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#Liste de chien par ordre décroissant d'âge en années
SELECT DISTINCT ?itemLabel ?age ?item WHERE {?item wdt:P31/wdt:P279* wd:Q144 ;
                                                   wdt:P569 ?naissance ;
                                                   wdt:P570 ?mort .
        FILTER(?age > 19) .
        BIND((?mort - ?naissance)/365.2425 as ?age )
    SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". } }
    ORDER BY DESC (?age)""";
