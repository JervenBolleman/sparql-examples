<https://www.wikidata.org/#query-61989359d0c4f6eff303051715f7bb26> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT DISTINCT ?item ?itemLabel ?itemDescription ?formatter ?languageLabel WHERE {
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en-GB,en,mul\". }
  {
    SELECT DISTINCT ?item ?formatter ?language WHERE { 
      ?item wdt:P1630 ?for.
      MINUS {?item wdt:P31 ?instance;
      p:P1630 [ ps:P1630 ?formatter ; pq:P407 ?language].}
      ?item wdt:P17 wd:Q145.
    }
  }
}""";
