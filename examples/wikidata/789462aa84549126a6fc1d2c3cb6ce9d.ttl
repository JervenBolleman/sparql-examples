@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix schema: <https://schema.org/> .

<https://www.wikidata.org/#query-789462aa84549126a6fc1d2c3cb6ce9d> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#----------------------------------------------------------
# Personen mit Arbeitgeber TU-Dresden ohne deutsche Beschreibung
#----------------------------------------------------------
SELECT ?item ?itemLabel ?itemDescription
WHERE 
{
  ?item wdt:P108 wd:Q158158
  filter not exists {?item schema:description ?desc . filter(lang(?desc)=\"de\") }
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],de,en\". }
}""";
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Stefan_K%C3%BChn/Dresden>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/> .
