<https://www.wikidata.org/#query-38d2183f65c01592f92358ef11d3b339> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?item ?itemLabel ?person ?personLabel ?relationship ?relationshipLabel
WHERE 
{
  VALUES ?item {wd:Q231270}
  ?item p:P1038 ?stat . 
  ?stat ps:P1038 ?person .
  ?stat pq:P1039 ?relationship .
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". } 
}""";
