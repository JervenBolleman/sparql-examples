ex:sparqlrdf/Wikidata:WikiProject_France/Queries/Langues/snippet_2.ttl a sh:SPARQLExecutable,
        sh:SPARQLSelectExecutable ;
    rdfs:comment "Sparql query from file sparqlrdf/Wikidata:WikiProject_France/Queries/Langues/snippet_2.ttl"^^rdf:HTML ;
