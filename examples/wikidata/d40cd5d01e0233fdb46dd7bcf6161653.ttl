<https://www.wikidata.org/#query-d40cd5d01e0233fdb46dd7bcf6161653> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?item ?itemLabel ?inst_subpLabel ?inst_subp_dc ?classLabel 
WHERE {
  ?class wdt:P279* wd:Q2615082 . 
  ?inst_subp wdt:P1647? wd:P31 .
  ?inst_subp wikibase:directClaim ?inst_subp_dc .
  ?item ?inst_subp_dc ?class .
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}""";
