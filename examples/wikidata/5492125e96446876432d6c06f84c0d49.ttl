<https://www.wikidata.org/#query-5492125e96446876432d6c06f84c0d49> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?bhl ?item ?val ?reasonLabel WHERE {
      ?item wdt:P4327 ?bhl .      
      ?item p:P1144 ?stmt .
      ?stmt ps:P1144 ?val .
      OPTIONAL {?stmt pq:P2241 ?reason} .
          
      SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
} ORDER BY xsd:integer(?bhl)""";
