<https://www.wikidata.org/#query-5d880b63f5f5cac1967ec2a35e26bbd9> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#Focus list finder for main subjects that are article items
SELECT DISTINCT ?item ?subjectLabel
   WHERE {?item wdt:P921 ?subject;
                wdt:P5008 wd:Q55439927.
          ?subject wdt:P31 wd:Q13442814.

          SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
         }
LIMIT 100""";
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:ScienceSource_project/Followups>;
