<https://www.wikidata.org/#query-a294c5cbf18f637a0f3c1b77ac8e2663> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT (COUNT(?disease) AS ?total) (SUM(?ref) AS ?byDO) (100*?byDO/?total AS ?percent)
WHERE
{
  {?disease wdt:P31 wd:Q12136 }
  UNION
  {?disease wdt:P279 wd:Q12136 .}
  OPTIONAL {
    ?disease p:P699 ?statement.
    BIND(1 AS ?ref).
  }
}""";
