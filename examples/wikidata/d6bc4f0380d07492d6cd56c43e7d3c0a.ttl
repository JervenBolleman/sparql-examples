@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix schema: <https://schema.org/> .

<https://www.wikidata.org/#query-d6bc4f0380d07492d6cd56c43e7d3c0a> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?pm ?pmLabel ?start_date ?end_date (SAMPLE(?party) AS ?party) (SAMPLE(?partyLabel) AS ?partyLabel) WHERE {
  ?pm p:P39 ?statement.
  ?statement ps:P39 wd:Q1587677;
    pq:P580 ?start_date.
  OPTIONAL { ?statement pq:P582 ?end_date. }
  OPTIONAL { ?pm wdt:P102 ?party. }
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}
GROUP BY ?pm ?pmLabel ?start_date ?end_date
ORDER BY DESC (?start_date)
LIMIT 7""";
  rdfs:comment " Here is the SPARQL query and the result for the last seven French prime ministers, including their political party: SPARQL Query: "@en;
