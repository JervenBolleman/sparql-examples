  sh:select """#US gymnastics
SELECT DISTINCT ?item ?itemLabel ?usaid ?usid 
WHERE 
{
  ?item wdt:P31 wd:Q5 .
  { ?item wdt:P4668 ?usaid . OPTIONAL { ?item wdt:P4583 ?usid } }
