<https://www.wikidata.org/#query-63f80ff6d931fd8b22fe8c295f0c973b> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT DISTINCT ?item WHERE {
  ?item wdt:P31 wd:Q11424 ;
        wdt:P31 ?film ;
        rdfs:label ?label .
  ?item2 wdt:P31 wd:Q571 ;
         wdt:P31 ?film2 ;
         rdfs:label ?label2
  filter(lang(?label) = \"fr\") .
  filter(lang(?label2) = \"fr\") .
  filter(?label = ?label2) .
  minus{?item wdt:P144 ?base .}
  minus{?item2 wdt:P4969 ?derive .}
}
LIMIT 1000""";
