<https://www.wikidata.org/#query-40f0568e58467ef7d0d89eea56d68499> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#articles scientifiques non associé à une publication, possédant un identifiant NASA ADS, avec la partie de l'identifiant ADS désignant le code du périodique concerné
SELECT distinct ?item ?label ?identifiant
WHERE {
  ?item wdt:P31 wd:Q13442814 ;
        rdfs:label ?label ;
        wdt:P819 ?ads ;
bind(STR(?ads) as ?string)
BIND (REPLACE (?string, \".*?([^0-9]*?)\\\\..*$\", \"$1\") AS ?identifiant)
MINUS{?item wdt:P1433 ?pubs .}
}
limit 1000""";
