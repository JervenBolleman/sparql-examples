@prefix schema: <https://schema.org/> .

<https://www.wikidata.org/#query-b27c552dbc62b8cf29cb521fb4e71209> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """# geeft een selectie van de sitelink aan, \";\" notatie
#title: Items met een site-link naar Wikispecies
SELECT ?item ?itemLabel ?article
WITH {
  SELECT *
  WHERE {
    ?article schema:about ?item ;
      schema:isPartOf <https://species.wikimedia.org/> .
  }
  LIMIT 200
} AS %i
WHERE {
  INCLUDE %i
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],mul,en\" . }
}""";
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:SPARQL_query_service/queries/examples/nl>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/> .
