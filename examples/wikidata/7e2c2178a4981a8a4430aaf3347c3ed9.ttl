@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix schema: <https://schema.org/> .

<https://www.wikidata.org/#query-7e2c2178a4981a8a4430aaf3347c3ed9> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT
  ?count
  ?identifier ?identifierLabel ?identifierDescription
WITH {
  SELECT 
    (COUNT(*) AS ?count)
    ?property 
  WHERE {
    ?property ^wikibase:directClaim / wdt:P31 wd:Q56216056 .
    [] ?property [] .
  }
  GROUP BY ?property
} AS %properties
WHERE {
  INCLUDE %properties
  ?identifier wikibase:directClaim ?property .
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}
ORDER BY DESC(?count)""";
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:Synia:lexeme-index>;
