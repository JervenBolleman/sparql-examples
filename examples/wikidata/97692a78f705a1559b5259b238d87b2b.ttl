<https://www.wikidata.org/#query-97692a78f705a1559b5259b238d87b2b> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?form ?representation WHERE {
  ?lexemeId dct:language wd:Q6654;
    ontolex:lexicalForm ?form.
  ?form ontolex:representation ?representation ;
    wikibase:grammaticalFeature wd:Q146233 .
  
  filter not exists {
   ?form ontolex:representation ?representation ;
    wikibase:grammaticalFeature ?unwanted.
    filter(?unwanted !=wd:Q146233 )
            }
}""";
