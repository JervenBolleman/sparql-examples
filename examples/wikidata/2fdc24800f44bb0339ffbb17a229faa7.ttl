<https://www.wikidata.org/#query-2fdc24800f44bb0339ffbb17a229faa7> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?person ?personLabel {
  BIND(wd:Q17 AS ?country)
  ?person wdt:P31 wd:Q5 .
  ?person wdt:P27 ?country . 
  # ?person wdt:P106 wd:Q191633 . # mangaka
  # ?person wdt:P106 wd:Q644687 . # illustrator
  OPTIONAL {?person wdt:P21 ?gender} FILTER (!bound(?gender))
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en,de,fr,es,ru,ca,it,ja,ar,zh,be,bg,bn,cs,da,el,et,fa,fi,he,hi,hu,hy,id,jv,ko,nb,nl,eo,pa,pl,pt,ro,sh,sk,sr,sv,sw,te,th,tr,uk,yue,vec,vi\" } 
} limit 100""";
