<https://www.wikidata.org/#query-9f2b1d85db5887867f4e801b2e90551b> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?item ?itemLabel WHERE {
  ?item wdt:P31 wd:Q5 .
  ?item wdt:P734  wd:Q16479693  . # Family name Badger
  ?item wdt:P735 ?name .
  ?name rdfs:label ?nameLabel .
  FILTER (LANG(?nameLabel) = \"en\")
  FILTER (STRSTARTS(?nameLabel, \"E\")) # First initial
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}""";
