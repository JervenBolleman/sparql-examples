<https://www.wikidata.org/#query-0cc758504599c7a1779a4b8697dc1afa> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#title:Partigiani morti in Toscana
SELECT ?partigiano ?partigianoLabel ?partigianoDescription ?luogonascitaLabel ?luogomorteLabel ?ANPI
WHERE {
  ?partigiano wdt:P106 wd:Q212948 .
  OPTIONAL { ?partigiano wdt:P19 ?luogonascita } .
  ?partigiano wdt:P20 ?luogomorte . ?luogomorte wdt:P131* wd:Q1273 .
  OPTIONAL { ?partigiano wdt:P9267 ?ANPI } .
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}
ORDER BY xsd:integer(?ANPI)""";
