@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix schema: <https://schema.org/> .

<https://www.wikidata.org/#query-a91eba992f3ab3774bc454cf162170e3> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?item ?label WHERE{
  ?item wdt:P31 wd:Q16521;
        wdt:P225 ?taxonname;
        rdfs:label ?label;
  FILTER(LANG(?label) = 'fr')
  FILTER(lcase(STR(?label))!= lcase(?taxonname))
  OPTIONAL {
    ?item wdt:P1843 ?commonname .
    FILTER(LANG(?commonname) = 'fr')
  }
  FILTER(!bound(?commonname))
} LIMIT 1000""";
