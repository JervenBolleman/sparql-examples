<https://www.wikidata.org/#query-14b0840e654febc5110ca4940032a040> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?item ?taxonName ?taxonRankLabel ?parentTaxon WHERE {
    ?item wdt:P31 wd:Q855769 .
    OPTIONAL {?item wdt:P225 ?taxonName} .
    OPTIONAL {?item wdt:P105 ?taxonRank} .
    OPTIONAL {?item wdt:P171/wdt:P225 ?parentTaxon} .
    SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE]\". }
}
ORDER BY ASC (?taxonName)""";
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Succu/SPARQL>;
