<https://www.wikidata.org/#query-a48a24a292607f36a01ae8113fa6e5ee> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?game ?gameLabel ?enwiki ?devLabel ?publisherLabel ?date WHERE {
	?game wdt:P31 wd:Q7889;
	wdt:P136 wd:Q744038.
    OPTIONAL {
      ?enwiki schema:about ?game;
      schema:isPartOf <https://en.wikipedia.org/>
    }
#    OPTIONAL {
      ?game wdt:P178 ?dev.
      ?game wdt:P123 ?publisher.
      ?game wdt:P577 ?date.
#    }
 	SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\". }
} ORDER BY ASC (?date)""";
