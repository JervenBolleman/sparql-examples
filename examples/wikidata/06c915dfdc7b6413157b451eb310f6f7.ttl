<https://www.wikidata.org/#query-06c915dfdc7b6413157b451eb310f6f7> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?person ?personLabel ?emlo ?causeLabel  WHERE {
?person wdt:P1802 ?emlo;
  wdt:P509 ?cause
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\" }
}
ORDER BY ?causeLabel""";
