<https://www.wikidata.org/#query-4d3b922719d8d94336da13740fbf7325> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?person ?personLabel ?positionLabel
WHERE {
  ?person p:P39 [a wdno:P155; ps:P39 ?position].             
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\". }
}""";
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:Request_a_query/Archive/2021/01>;
