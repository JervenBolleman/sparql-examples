<https://www.wikidata.org/#query-3eabe8167a9842359afc63012d2573e1> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?item ?itemLabel ?lcsh
WHERE {
  ?item wdt:P219 ?iso.
  OPTIONAL {?item wdtn:P244 ?lcsh}
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}
ORDER BY ?itemLabel""";
  rdfs:comment "Properties: ISO 639-2 code (P219)   , Library of Congress authority ID (P244)   "@en;
