<https://www.wikidata.org/#query-1231f8e4280611f8f00cf146919fcba4> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?name ?nameLabel ?count WHERE {
  {
    SELECT ?name (COUNT(?person) AS ?count) WHERE {
      ?person wdt:P31 wd:Q5;
              wdt:P27 wd:Q36;
              wdt:P21 wd:Q6581097;
              wdt:P735 ?name.
    }
    GROUP BY ?name
  }
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\", \"pl\". }
}
ORDER BY DESC(?count)
LIMIT 100""";
