ex:sparqlrdf/Wikidata:WikiProject_Manuscripts/Data_Model/snippet_1.ttl a sh:SPARQLExecutable,
        sh:SPARQLSelectExecutable ;
    rdfs:comment "Sparql query from file sparqlrdf/Wikidata:WikiProject_Manuscripts/Data_Model/snippet_1.ttl"^^rdf:HTML ;
