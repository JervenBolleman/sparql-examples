<https://www.wikidata.org/#query-d46ab074d416fc9d20398705bc09bb54> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?item ?itemLabel ?websiteLabel WHERE {
  ?item wdt:P856 ?website.
  FILTER CONTAINS(LCASE(str(?website)), 'bbc.com' ).
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}
LIMIT 10""";
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:Request_a_query/Archive/2022/07>;
