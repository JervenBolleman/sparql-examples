<https://www.wikidata.org/#query-4a5188d61900927dce7ff7a547034554> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#What entrys are using URL match pattern and which contain VIAF
SELECT ?item ?itemLabel ?value
WHERE 
{
  ?item wdt:P8966 ?value
     FILTER (CONTAINS(lcase(?value), \"viaf\"))   
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". } # Helps get the label in your language, if not, then en language
}""";
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata_talk:Tools/Wikidata_for_Web>,
    <https://www.wikidata.org//wiki/User:Back_ache>;
