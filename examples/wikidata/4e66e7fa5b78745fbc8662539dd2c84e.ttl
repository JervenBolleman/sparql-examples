  sh:select """SELECT
  ?país
  ?paísLabel
  ?instancia_deLabel
  ?cosa
  ?cosaLabel
  ?peruano
  ?peruanoLabel
{
  {?peruano wdt:P27 wd:Q419 }
  UNION
  {?peruano wdt:P19/wdt:P131* wd:Q419 }

  ?cosa wdt:P138 ?peruano;
        wdt:P17 ?país;
        wdt:P31 ?instancia_de.
  
  FILTER(?país != wd:Q419)
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"es\". }
}
ORDER BY ?paísLabel""";
