PREFIX bds: <http://www.bigdata.com/rdf/search#>
PREFIX gas: <http://www.bigdata.com/rdf/gas#>
PREFIX hint: <http://www.bigdata.com/queryHints#>

#Romanian settlement with wrong diacritics in non-ro and non-tr labels
SELECT ?item ?itemLabel ?itemLabelFixed4 (LANG(?itemLabel) AS ?itemLabelLang)
WHERE 
{
  ?item (wdt:P31/wdt:P279*) wd:Q697379.
  ?item rdfs:label ?itemLabel.
  #SERVICE wikibase:label { bd:serviceParam wikibase:language "[AUTO_LANGUAGE],en". } hint:Prior hint:runLast false.
  FILTER(LANG(?itemLabel) NOT IN ("ro", "tr"))
  FILTER(REGEX(?itemLabel, "[ŞşŢţ]"))
  BIND(REPLACE(?itemLabel, "Ş", "Ș") AS ?itemLabelFixed1)
  BIND(REPLACE(?itemLabelFixed1, "ş", "ș") AS ?itemLabelFixed2)
  BIND(REPLACE(?itemLabelFixed2, "Ţ", "Ț") AS ?itemLabelFixed3)
  BIND(REPLACE(?itemLabelFixed3, "ţ", "ț") AS ?itemLabelFixed4)
}
#ORDER BY ?itemLabelLang

""" ;
    schema:target <https://query.wikidata.org/sparql/> .
