<https://www.wikidata.org/#query-a622ccdacc573670f42eae9b564ef6cc> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?item ?itemLabel ?roleValue ?roleValueLabel WITH {
  SELECT ?item ?roleValue
  WHERE {
  ?item wdt:P2868 ?roleValue.
    } } as %i
WHERE
{
  INCLUDE %i
  ?item wdt:P31 wd:Q16521 .
  SERVICE wikibase:label { bd:serviceParam wikibase:language 'en' }
  }""";
