  sh:select """SELECT (COUNT(DISTINCT ?item) AS ?count) WHERE {
  ?item wdt:P1343 wd:Q46002746; wdt:P31 wd:Q5 .
  [] schema:about ?item; schema:isPartOf ?project .
  FILTER(CONTAINS(STR(?project), 'wikipedia')) .
}""";
