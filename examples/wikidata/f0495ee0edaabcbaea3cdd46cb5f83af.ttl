<https://www.wikidata.org/#query-f0495ee0edaabcbaea3cdd46cb5f83af> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?Museum ?MuseumLabel ?MuseumIDLabel ?locationLabel ?MuseenID ?GKZ WHERE {

  ?Museum p:P7858 ?NMIDstmt. 
  ?NMIDstmt ps:P7858 ?MuseenID.
  OPTIONAL { ?NMIDstmt pq:P1932 ?MuseumIDLabel. }
  ?Museum wdt:P131 ?location.
  ?location wdt:P964 ?GKZ.
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
<nowiki>}</nowiki>""";
