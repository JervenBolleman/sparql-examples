<https://www.wikidata.org/#query-4bf9d7a8ed05dfba75d87fc56089bb24> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?lang ?langLabel (COUNT(DISTINCT ?website) as ?count)
WHERE {
  ?lang wdt:P31/wdt:P279* wd:Q9143.
  ?lang p:P856 ?website.
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\" }
}
GROUP BY ?lang ?langLabel
ORDER BY DESC(?count)""";
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:Request_a_query/Archive/2017/03>,
    <https://www.wikidata.org//wiki/User:Infovarius/Queries>;
