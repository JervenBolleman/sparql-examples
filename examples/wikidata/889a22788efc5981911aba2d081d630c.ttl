<https://www.wikidata.org/#query-889a22788efc5981911aba2d081d630c> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#List of all non recognised countries
SELECT ?country ?countryLabel
WHERE {
  ?country wdt:P31/wdt:P279* wd:Q15634554.
  #?country wdt:P31 wd:Q6256.
  MINUS{?country wdt:P31/wdt:P279* wd:Q3024240.}
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\". }
}
GROUP BY ?country ?countryLabel ?country2 ?country2Label
ORDER BY DESC(?count)""";
