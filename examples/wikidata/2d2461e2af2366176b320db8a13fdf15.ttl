<https://www.wikidata.org/#query-2d2461e2af2366176b320db8a13fdf15> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT DISTINCT ?movieLabel ?movie ?personLabel ?person ?productionCompanyLabel ?productionCompany
WHERE {
  ?movie wdt:P272 ?productionCompany;
         wdt:P161 ?person ;
         rdfs:label ?movieLabel.
  ?productionCompany wdt:P112 ?person .
#  ?prop wikibase:directClaim ?p
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\" }
}
ORDER BY ?movieLabel
LIMIT 100""";
  rdfs:comment "Properties: production company (P272)   , cast member (P161)   , founded by (P112)   "@en;
