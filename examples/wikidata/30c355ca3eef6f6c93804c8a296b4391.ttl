<https://www.wikidata.org/#query-30c355ca3eef6f6c93804c8a296b4391> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT DISTINCT ?watercraft (REPLACE(STR(?watercraft),\".*Q\",\"Q\") AS ?watercraftQid) ?watercraftLabel ?watercraftTypeLabel
WHERE
{
  # Plain watercraft
  ?watercraft wdt:P31/wdt:P279* wd:Q1229765 .
  # Also some watercraft type
  ?watercraft wdt:P31 ?watercraftType .
  ?watercraftType wdt:P279* wd:Q16335899 .
  SERVICE wikibase:label { 
    bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en,fr,es,it,de,pl\".
  }
}
LIMIT 50""";
