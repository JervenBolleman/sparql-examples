<https://www.wikidata.org/#query-5dc3f02e7544b341bea58f3f1493f067> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#title:NUKAT appearing two times equal in two different items (= unique-value violation)
SELECT DISTINCT ?v1 ?item1 ?item2
WHERE { 
  ?item1 p:P1207 ?s1 .
  ?s1 ps:P1207 ?v1 .
  ?item2 p:P1207 ?s2 .
  ?s2 ps:P1207 ?v2 .
  FILTER(?item1 != ?item2)
  FILTER(?v1 = ?v2)
}
LIMIT 10""";
