@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix schema: <https://schema.org/> .

<https://www.wikidata.org/#query-6bca7df91952945c55fcf4ef24b8e185> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """# Items in Lithuania that have coordinates missing an image
SELECT ?item ?itemLabel ?coord
WHERE
{
	?item wdt:P17 wd:Q37 .
    ?item wdt:P625 ?coord .
    FILTER NOT EXISTS {
        ?item wdt:P18 ?image
    } 
	SERVICE wikibase:label { bd:serviceParam wikibase:language \"lt\" }
}""";
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:WikiProject_Lithuania/Maintenance_Queries>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/> .
