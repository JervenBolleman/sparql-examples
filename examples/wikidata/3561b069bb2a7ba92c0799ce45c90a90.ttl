<https://www.wikidata.org/#query-3561b069bb2a7ba92c0799ce45c90a90> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT DISTINCT ?item ?itemLabel ?coord
WHERE 
{
  ?item wdt:P381 ?refnr . # PCP Reference number (KGS Nummer)
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
  FILTER(?refnr IN (\"2806\")  )
}""";
