<https://www.wikidata.org/#query-4df7da1c69c7a455b7579db9c349d18c> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT DISTINCT ?person ?personLabel ?gndID WHERE {
  {
    ?person (wdt:P31+/(wdt:P279*)) wd:Q1114461.
    ?person wdt:P227 ?gndID .
  } SERVICE wikibase:label { bd:serviceParam wikibase:language \"de,en\". }
} ORDER BY ASC(?personLabel)""";
