@prefix schema: <https://schema.org/> .

<https://www.wikidata.org/#query-e81980c36c695f9abf9417fbcc2698e7> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?subject ?subjectLabel ?count WHERE {
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
  {
    SELECT ?subject (COUNT(?item) as ?count) WHERE {
      ?item p:P31 ?statement0.
      ?statement0 (ps:P31/(wdt:P279*)) wd:Q7889.
      ?item wdt:P921 ?subject.
    }
    GROUP BY ?subject
  }
}
ORDER BY DESC(?count)""";
