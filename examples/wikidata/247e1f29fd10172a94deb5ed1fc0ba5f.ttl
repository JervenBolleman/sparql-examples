@prefix schema: <https://schema.org/> .

<https://www.wikidata.org/#query-247e1f29fd10172a94deb5ed1fc0ba5f> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?class ?classLabel ?count WHERE {
  {
    SELECT ?class (COUNT(*) AS ?count) WHERE {
       ?a wdt:P31 wd:Q484170 .
       ?b wdt:P31 ?class .
       ?a wdt:P47 ?b .
    } GROUP BY ?class
  } . 
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"fr\" . }
}
ORDER BY DESC(?count) ?classLabel""";
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata_talk:WikiProject_France>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/> .
