<https://www.wikidata.org/#query-4c92fbd3f1146f45aa75d33e3a9d71cf> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#title:Number of researchers with affiliation to institutions in Switzerland or Liechtenstein by gender
SELECT ?gender ?genderLabel (COUNT(DISTINCT ?person) AS ?number)
WHERE {
  ?person wdt:P108 [ wdt:P17 ?country ] .
  ?person wdt:P21 ?gender .
  VALUES ?country { wd:Q39 wd:Q347 } .
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\". }
}
GROUP BY ?gender ?genderLabel ?number""";
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:WikiCite/Researchers_in_Switzerland/Queries>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/> .
