<https://www.wikidata.org/#query-41fa4055e39b4496084395a02a42b150> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?project ?projectLabel ?inception ?use ?useLabel
WHERE 
{
  ?project wdt:P31/wdt:P279* wd:Q106323251.
  
  OPTIONAL {?project wdt:P571 ?inception}
  OPTIONAL {?project wdt:P366 ?use}

  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
} ORDER BY ?projectLabel""";
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:Topics/Wikidata/Userscripts>;
