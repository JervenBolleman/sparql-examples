  VALUES ?country {wd:Q40 wd:Q39 wd:Q183}
  ?item wdt:P31 wd:Q7725634.
  { # Author was born in ?country
    ?item wdt:P50 ?author.
    ?author wdt:P19 ?place.
    ?place wdt:P17 ?country.
  }
  UNION
  { # Author has ?country citizenship
    ?item wdt:P50 ?author.
    ?author wdt:P27 ?country.
  }
  UNION
  { # First performance was in ?country
    ?item wdt:P4647 ?firstperformance.
    ?firstperformance wdt:P17 ?country.
  }
