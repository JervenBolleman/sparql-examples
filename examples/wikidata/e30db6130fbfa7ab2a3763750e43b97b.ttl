@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix schema: <https://schema.org/> .

<https://www.wikidata.org/#query-e30db6130fbfa7ab2a3763750e43b97b> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#recherche un élément portant le no d'un identifiant externe pour lequel la propriété existe dans Wikidata
SELECT ?item ?itemLabel ?itemDescription
WHERE
{
   ?item wdt:P268 ?id. #propriété correspondant à l'identifiant d'autorité dans le catalogue de la Bibliothèque nationale de France
  FILTER ( STR(?id) = '123735831') #no recherché
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}""";
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata_talk:Events/GLAM_%26_Wikimedia_CH>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/> .
