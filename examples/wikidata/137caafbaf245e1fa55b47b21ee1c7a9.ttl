<https://www.wikidata.org/#query-137caafbaf245e1fa55b47b21ee1c7a9> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT distinct ?drug ?drugLabel ?value ?valueLabel ?reference_stated_inLabel  WHERE {
  ?drug p:P2175 ?statement .
  ?statement ps:P2175 ?value .  # get the value associated with the statement
  ?statement prov:wasDerivedFrom/pr:P248 ?reference_stated_in . #where stated
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\" . }
}""";
  rdfs:comment "Properties: medical condition treated (P2175)   , stated in (P248)   "@en;
