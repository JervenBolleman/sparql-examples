@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix schema: <https://schema.org/> .

<https://www.wikidata.org/#query-0b968c243ca0fd3ba64b9990d46fd759> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#title:Count of manuscripts by collection
SELECT ?collection ?collectionLabel (COUNT(?item) AS ?count) WHERE {
  ?item wdt:P31/wdt:P279* wd:Q87167.
  ?q p:P195 ?s. ?s ps:P195 ?collection . MINUS {?s pq:P582 []}.
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
} GROUP BY ?collection ?collectionLabel
ORDER BY DESC(?count)""";
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:WikiProject_Manuscripts/Data_Model>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/> .
