@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix schema: <https://schema.org/> .

<https://www.wikidata.org/#query-b4bbda1738a0c864a1012490efa8e041> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?studytype ?studytypeLabel (count(?study) AS ?number)
WHERE {
  ?study wdt:P31 wd:Q30612 .
  ?study wdt:P8363 ?studytype . 
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"de,en\" }
} GROUP BY ?studytype ?studytypeLabel
ORDER BY DESC(?number)""";
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Wiljes/Clinical-Trials>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/> .
