@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix schema: <https://schema.org/> .

<https://www.wikidata.org/#query-79ef2d7eb40bc7b2e729c583a16c2394> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """PREFIX target: <http://www.wikidata.org/entity/Q308915>

SELECT
  ?count
  ?class ?classLabel
  ?classDescription
 
  ?example_work ?example_workLabel
  (CONCAT(\"#work/\", SUBSTR(STR(?example_work), 32)) AS ?example_workUrl)
WITH {
  SELECT 
    (COUNT(*) AS ?count)
    ?class
    (SAMPLE(?work) AS ?example_work)
  WHERE {
    ?work wdt:P31 ?class ;
          wdt:P275 target: .
  } 
  GROUP BY ?class
} AS %classes
WHERE {
  INCLUDE %classes
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}
ORDER BY DESC(?count)""";
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:Synia:license>;
