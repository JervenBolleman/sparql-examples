<https://www.wikidata.org/#query-e4dc2e2f11f9b5fd2af96fc9673ae608> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?item ?itemLabel ?gouvernement ?gouvernementLabel 
WHERE {
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"fr\". }
  ?item wdt:P463 ?gouvernement.
  ?gouvernement wdt:P31 wd:Q1450662.
}
ORDER BY ?gouvernementLabel ?itemLabel""";
