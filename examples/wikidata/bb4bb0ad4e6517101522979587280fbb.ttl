<https://www.wikidata.org/#query-bb4bb0ad4e6517101522979587280fbb> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?item ?label (LANG(?label) AS ?lang) WHERE {
  ?item wdt:P31 wd:Q5 .
  ?item wdt:P106 wd:Q33999 .
  ?item rdfs:label ?label .
  FILTER(LANG(?label) = \"de\") .
  FILTER(STRENDS(?label, \" (Schauspieler)\")) .
}""";
