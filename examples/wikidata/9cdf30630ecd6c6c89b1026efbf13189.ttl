@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix schema: <https://schema.org/> .

<https://www.wikidata.org/#query-9cdf30630ecd6c6c89b1026efbf13189> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#no description
SELECT ?item ?itemLabel ?itemLabel_ar WITH {
  SELECT ?item WHERE {
    ?item wdt:P106 wd:Q937857 .
    filter not exists { ?item rdfs:label ?itemDesc_ar . filter(lang(?itemDesc_ar)=\"ar\") }
  } LIMIT 200
} AS %RESULTS {
  INCLUDE %RESULTS
  optional {?item schema:description ?itemLabel_ar . filter(lang(?itemLabel_ar)=\"ar\")}
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}""";
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Mohammad_Hijjawi/SPARQL_queries>;
