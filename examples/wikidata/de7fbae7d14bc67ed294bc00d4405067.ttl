<https://www.wikidata.org/#query-de7fbae7d14bc67ed294bc00d4405067> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?artist ?artistLabel (URI(CONCAT(\"http://vocab.getty.edu/page/ulan/\",?getty_id)) AS ?getty_link) (COUNT(?work) AS ?count) WHERE {
?work wdt:P195 wd:Q636400; wdt:P170 ?artist FILTER (!ISBLANK(?artist)) .
?artist wdt:P245 ?getty_id
SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
} GROUP BY ?artist ?artistLabel ?getty_id
ORDER BY ?artistLabel""";
