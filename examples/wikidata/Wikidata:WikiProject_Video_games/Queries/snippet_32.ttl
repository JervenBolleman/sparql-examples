ex:sparqlrdf/Wikidata:WikiProject_Video_games/Queries/snippet_32.ttl a sh:SPARQLExecutable,
        sh:SPARQLSelectExecutable ;
    rdfs:comment "Sparql query from file sparqlrdf/Wikidata:WikiProject_Video_games/Queries/snippet_32.ttl"^^rdf:HTML ;
