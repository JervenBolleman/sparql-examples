  { ?item wdt:P31 wd:Q241212. }
  UNION
  { ?item wdt:P31 wd:Q3851512. }
  optional {?item wdt:P131 ?location. optional {?location wdt:P131 ?municipality . } } 
  ?item wdt:P17 wd:Q45.
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"pt\" }
