ex:sparqlrdf/Wikidata:Request_a_query/Archive/2018/08/snippet_64.ttl a sh:SPARQLExecutable,
        sh:SPARQLSelectExecutable ;
    rdfs:comment "Sparql query from file sparqlrdf/Wikidata:Request_a_query/Archive/2018/08/snippet_64.ttl"^^rdf:HTML ;
