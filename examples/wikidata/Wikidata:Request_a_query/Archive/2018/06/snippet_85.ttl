ex:sparqlrdf/Wikidata:Request_a_query/Archive/2018/06/snippet_85.ttl a sh:SPARQLExecutable,
        sh:SPARQLSelectExecutable ;
    rdfs:comment "Sparql query from file sparqlrdf/Wikidata:Request_a_query/Archive/2018/06/snippet_85.ttl"^^rdf:HTML ;
