ex:sparqlrdf/Wikidata:Request_a_query/Archive/2020/03/snippet_4.ttl a sh:SPARQLExecutable,
        sh:SPARQLSelectExecutable ;
    rdfs:comment "Sparql query from file sparqlrdf/Wikidata:Request_a_query/Archive/2020/03/snippet_4.ttl"^^rdf:HTML ;
