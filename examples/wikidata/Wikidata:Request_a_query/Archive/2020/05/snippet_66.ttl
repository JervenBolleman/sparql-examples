ex:sparqlrdf/Wikidata:Request_a_query/Archive/2020/05/snippet_66.ttl a sh:SPARQLExecutable,
        sh:SPARQLSelectExecutable ;
    rdfs:comment "Sparql query from file sparqlrdf/Wikidata:Request_a_query/Archive/2020/05/snippet_66.ttl"^^rdf:HTML ;
