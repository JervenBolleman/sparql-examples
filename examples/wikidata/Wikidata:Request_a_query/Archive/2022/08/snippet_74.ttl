ex:sparqlrdf/Wikidata:Request_a_query/Archive/2022/08/snippet_74.ttl a sh:SPARQLExecutable,
        sh:SPARQLSelectExecutable ;
    rdfs:comment "Sparql query from file sparqlrdf/Wikidata:Request_a_query/Archive/2022/08/snippet_74.ttl"^^rdf:HTML ;
