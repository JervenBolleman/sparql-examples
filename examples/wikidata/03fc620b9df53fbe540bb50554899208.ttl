  sh:select """SELECT ?s ?label (LANG(?label) AS ?lc)
Where {
    ?s ?p \"Smithsonian Institution\"@zh .
    ?s rdfs:label ?label . FILTER (langmatches(lang(?label), \"zh\"))
}
LIMIT 100""";
