<https://www.wikidata.org/#query-b6a01f096b5fffe9c2b8ccb6065a1ae9> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?bib ?bibLabel ?orgLabel ?schluessel WHERE
{
    ?bib wdt:P31/wdt:P279* wd:Q7075 . # Bibliotheken (inkl. Unterarten)
    ?bib wdt:P131+ ?org .             # Administrativ übergeordnete Einheit (transitiv)
    ?org wdt:P1388 ?schluessel .      # Regionalschlüssel
    SERVICE wikibase:label { bd:serviceParam wikibase:language \"de\" }
}""";
