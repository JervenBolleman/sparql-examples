<https://www.wikidata.org/#query-4efc22afe807aa113686a8655b1955eb> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#For book properties
#defaultView:BubbleChart
SELECT ?property ?propertyLabel ?propertyDescription ?count WHERE { 
	{
		select ?propertyclaim (COUNT(*) AS ?count) where {
			?item wdt:P31 wd:Q571 .
			?item ?propertyclaim [] .
		} group by ?propertyclaim 
	}
	?property wikibase:claim ?propertyclaim .
	SERVICE wikibase:label { bd:serviceParam wikibase:language \"fr,en\" . }
} ORDER BY DESC (?count)""";
