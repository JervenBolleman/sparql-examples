<https://www.wikidata.org/#query-ffafa02deca37c357198c250964e64fe> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#title:P1114 statements with units
SELECT ?item ?itemLabel ?st ?val ?unit ?unitLabel WHERE {
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
  ?item p:P1114 ?st.
  ?st psv:P1114 ?v.
  ?v wikibase:quantityAmount ?val.
  ?v wikibase:quantityUnit ?unit.
  FILTER( ?unit != wd:Q199 )
}
ORDER BY ?item
LIMIT 1000""";
